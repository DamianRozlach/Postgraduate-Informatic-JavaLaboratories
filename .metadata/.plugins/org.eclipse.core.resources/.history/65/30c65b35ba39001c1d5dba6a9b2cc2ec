package lab1;

import java.util.Scanner;
import java.util.Arrays;

public class Zad4Dividers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int zmienna = readNumber();
		printDividers(findDividers(zmienna),zmienna);

	}
	public static int readNumber() {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Podaj liczbe ktorej dzielniki ma wypisac program");
		int liczba = scanner.nextInt();
		scanner.close();
		return liczba;
	}
	
	public static boolean checkDivider(int dzielna, int dzielnik) {
		int reszta = dzielna % dzielnik;
		if(reszta == 0) return true;
		else return false;
	}
	
	public static int[] findDividers(int liczba) {
		int[] dividers = new int[liczba/2 +1];
		int counter = 0;
		int x = 0;
		while(x*x<liczba) {
			if(checkDivider(liczba,x)) {
				dividers[counter]=x;
				counter++;
				dividers[counter]=liczba/x;
				counter++;
			}
		}
		if(x*x == liczba) {
			dividers[counter] = x;
		}
		Arrays.sort(dividers);
		return dividers;
		
	}
	
	public static void printDividers(int[] dividers,int liczba) {
		System.out.printf("Dzielniki liczby %d:",liczba);
		for(int i=0;i<dividers.length;i++){
			System.out.printf("%d ", dividers[i]);
		}
	}

}
